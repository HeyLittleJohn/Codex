    "version": "0.2.0",
    "configurations": [
        {
            "name": "Data Pipeline-CLI orchestrator",
            "type": "python",
            "request": "launch",
            "program": "~/option_bot/option_bot/data_pipeline/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args":["all_", "-aa"],
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }

        },
        {
            "name": "Generic Python Debug",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Polygon - Multiprocessing",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            },
            "args": [
                "-Xfrozen_modules=off"
            ],
            "justMyCode": true
        }
    ]
}